{
  "expo": {
    "name": "fullstach-stripe-app",
    "slug": "fullstach-stripe-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fullstach-stripe-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gatio.fullstachstripeapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gatio.fullstachstripeapp",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://fullstach-stripe-app.con"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.gatio.fullstachstripeapp",
          "publishableKey": "pk_test_51PWXqNCz1Ym9ePz8NZQykTGS1gngV6ntRXQjku20ziMQ0xxIpuXbxRm47AW1tTx0P0UWE6DjcBzbmUH6XVBdIr3600XvMeXPb3"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "backendUrl": "http://192.168.0.50:8080",
      "router": {
        "origin": false
      }
    }
  }
}
